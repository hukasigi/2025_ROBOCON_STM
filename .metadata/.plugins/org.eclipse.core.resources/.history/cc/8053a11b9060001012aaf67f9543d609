void CAN_TX(uint32_t recipient) {
    //ÈÄÅ‰ø°Áî®„Ç§„É≥„Çπ„Çø„É≥„ÇπÁ≠â
CAN_TxHeaderTypeDef TxHeader;
uint32_t TxMailbox;
uint8_t TxData[8];
    //ÈÄÅ‰ø°„É°„Éº„É´„Éú„ÉÉ„ÇØ„Çπ„Å´Á©∫„Åç„Åå„ÅÇ„Å£„Åü„ÇâÈÄÅ‰ø°ÈñãÂßã
if (0 < HAL_CAN_GetTxMailboxesFreeLevel(&hcan)) {
        //ÈÄÅ‰ø°Áî®„Ç§„É≥„Çπ„Çø„É≥„Çπ„ÅÆË®≠ÂÆö
TxHeader.StdId = recipient;// ÂèóÂèñÊâã„ÅÆCAN„ÅÆID
TxHeader.RTR = CAN_RTR_DATA;
TxHeader.IDE = CAN_ID_STD;
TxHeader.DLC = 8;//„Éá„Éº„ÇøÈï∑„Çí8byte„Å´Ë®≠ÂÆö
TxHeader.TransmitGlobalTime = DISABLE;
        //ÂêÑ„Éá„Éº„Çø
TxData[0] = 0;
TxData[1] = 1;
TxData[2] = 2;
TxData[3] = 3;
TxData[4] = 4;
TxData[5] = 5;
TxData[6] = 6;
TxData[7] = 7;
        //CAN„É°„ÉÉ„Çª„Éº„Ç∏„ÇíÈÄÅ‰ø°
if (HAL_CAN_AddTxMessage(&hcan, &TxHeader, TxData, &TxMailbox) != HAL_OK) {
Error_Handler();
}
}
}



