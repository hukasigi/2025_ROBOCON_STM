int id;
void HAL_CAN_RxFifo0MsgPendingCallback(CAN_HandleTypeDef *hcan1) {
CAN_RxHeaderTypeDef RxHeader;//Âèó‰ø°„É°„ÉÉ„Çª„Éº„Ç∏„ÅÆÊÉÖÂ†±„ÅåÊ†ºÁ¥ç„Åï„Çå„Çã„Ç§„É≥„Çπ„Çø„É≥„Çπ
uint8_t RxData[8];//Âèó‰ø°„Åó„Åü„Éá„Éº„Çø„Çí‰∏ÄÊôÇ‰øùÂ≠ò„Åô„ÇãÈÖçÂàó
if (HAL_CAN_GetRxMessage(hcan1, CAN_RX_FIFO0, &RxHeader, RxData)== HAL_OK) {
id = RxHeader.StdId; // RxHeader„ÅÆ‰∏≠„Å´ÂÖ•„Å£„Å¶„ÅÑ„Çãid„ÇíÂèñ„ÇäÂá∫„Åô
if (id == 0x001){
for (int i = 0; i <= 7; i++){
    use_data[i] = RxData[i];
}
}
}
}



