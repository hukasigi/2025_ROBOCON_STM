/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_USART2_UART_Init();
  MX_CAN_Init();
  /* USER CODE BEGIN 2 */
	/* USER CODE BEGIN 2 */

	// CAN„Çπ„Çø„Éº„Éà
	HAL_CAN_Start(&hcan);
	// Ââ≤„ÇäËæº„ÅøÊúâÂäπ
	HAL_CAN_ActivateNotification(&hcan, CAN_IT_RX_FIFO0_MSG_PENDING);
	/* USER CODE END 2 */

  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  uint32_t lastTick = HAL_GetTick();
  while (1)
  {
    /* USER CODE END WHILE */

   if(HAL_GetTick() - lastTick > 10000){
	   CAN_TX(0x001);
	   lastTick = HAL_GetTick();
   }
   printf("%d\r\n",use_data[0]);
   printf("%d\r\n",use_data[1]);
   printf("%d\r\n",use_data[2]);
   printf("%d\r\n",use_data[3]);
   printf("%d\r\n",use_data[4]);
   printf("%d\r\n",use_data[5]);
   printf("%d\r\n",use_data[6]);
   printf("%d\r\n",use_data[7]);

   HAL_DELAY(500);

    /* USER CODE BEGIN 3 */
  }

}



